# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=data_analytics
POSTGRES_PORT=5432

# Prisma Configuration
# Update these with your actual database connection string
DATABASE_URL=postgresql://buyer@localhost:5432/data_analytics?schema=public
DIRECT_DATABASE_URL=postgresql://buyer@localhost:5432/data_analytics?schema=public

# API Keys
SMARTMOVING_API_KEY=your_smartmoving_api_key_here
SMARTMOVING_API_URL=https://api-public.smartmoving.com/v1
GENDERIZE_API_KEY=your_genderize_api_key_here

# Environment
NODE_ENV=development
PYTHON_ENV=development

# Data Quality Monitoring
ALERT_EMAIL=admin@example.com
SLACK_WEBHOOK_URL=your_slack_webhook_url_here

# Splink Configuration
SPLINK_BLOCKING_RULE_CONFIDENCE_THRESHOLD=0.95
SPLINK_MANUAL_REVIEW_THRESHOLD=0.75

# Gender Determination
GENDERIZE_BATCH_SIZE=100
GENDERIZE_RATE_LIMIT_DELAY=1
